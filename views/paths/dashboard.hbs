 
<div>
<h4>PATH DASHBOARD</h4>

<div class="haiku-card">
    {{#each paths}}
    <div class="haiku-border" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">{{this.type}}</h5>
            <h6 class="card-subtitle mb-2 text-muted">{{this.title}}</h6>
            <p class="card-text">{{this.stage}}</p>
            <a href="#" class="card-link">Continue on this path</a>
            <a href="#" class="card-link">Delete</a>
        </div>
    </div>
    <br>
    {{/each}}
</div>


{{#if isFinished }}
    {{#each path.images}}

    {{/each}}
{{else }}
{{/if}}